// @Library('jenkins-libraries') _
library 'jenkins-libraries@main'
pipeline {
    agent none
    stages {
        stage ('log') {
            steps {
                // log.info 'Starting'
                script {
                    log.info 'Starting'
                    log.warning 'Nothing to do!'
                }
            }
        }

        stage ('sayHello') {
            steps {
                // log.info 'Starting'
                script {
                    sayHello 'Joe'
                }
            }
        }
    }
}